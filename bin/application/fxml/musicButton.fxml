<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="60.0" prefWidth="480.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="application.ButtonController">
  <children>
    <Button fx:id="btn" alignment="CENTER_LEFT" defaultButton="false" focusTraversable="false" layoutX="0.0" layoutY="0.0" mnemonicParsing="false" onAction="#playButtonAction" prefHeight="60.0" prefWidth="480.0" text="aaa" textAlignment="LEFT">
      <font>
        <Font size="20.0" />
      </font>
      <graphic>
        <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
          <effect>
            <DropShadow />
          </effect>
          <image>
            <Image url="@../img/musicFile.png" />
          </image>
        </ImageView>
      </graphic>
    </Button>
  </children>
</AnchorPane>
